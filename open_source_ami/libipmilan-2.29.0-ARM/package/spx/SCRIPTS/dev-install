		CWD %IMAGE_TREE/usr/local/lib/ipmi
		CREATE_SOFT_LINK ../libipmilan.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX libipmilan.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

