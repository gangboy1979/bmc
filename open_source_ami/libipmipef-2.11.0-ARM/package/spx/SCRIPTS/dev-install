# Install Script

build-install:
		MKDIR_NOCLEAN %SPXLIB/ipmipef
		COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmipef.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		COPY_FILE ./libipmipef.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/ipmipef
		ADD_LIBRARY_LINKS %SPXLIB/ipmipef libipmipef.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install:
