# Install Script

build-install:
	MKDIR_NOCLEAN %SPXLIB/interruptsensor
	COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
	ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmiinterruptsensor.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
	COPY_FILE ./libipmiinterruptsensor.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/interruptsensor
	ADD_LIBRARY_LINKS %SPXLIB/interruptsensor libipmiinterruptsensor.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install:
