# Install Script

build-install:
		COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmiapml.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		CWD %IMAGE_TREE/usr/local/lib/ipmi
		CREATE_SOFT_LINK ../libipmiapml.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX libipmiapml.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install:
