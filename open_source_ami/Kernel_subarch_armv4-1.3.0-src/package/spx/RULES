#Extract Source	in %SOURCE
extract_source:
		
#Build Source in %BUILD
build_source:
	INVOKE_SHELL ApplyPatch  %KERNEL_SRC/linux %SOURCE/%PACKAGE/data

#Clean Source
clean_source:
	DELETE %BINARY/@Kernel_subarch_armv4-*-ANY.spx

