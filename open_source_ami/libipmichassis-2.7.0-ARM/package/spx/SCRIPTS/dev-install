# Install Script

build-install:
		MKDIR_NOCLEAN %SPXLIB/ipmichassis
		COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmichassis.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		COPY_FILE ./libipmichassis.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/ipmichassis
		ADD_LIBRARY_LINKS %SPXLIB/ipmichassis libipmichassis.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install:
