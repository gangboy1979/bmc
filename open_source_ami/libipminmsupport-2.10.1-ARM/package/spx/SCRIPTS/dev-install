# Install Script

build-install:
          MKDIR_NOCLEAN %SPXLIB/ipminmsupport
          COPY_DIR                     ./ %IMAGE_TREE/usr/local/lib/
          ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib  libipminmsupport.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
          COPY_FILE ./libipminmsupport.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX  %SPXLIB/ipminmsupport
          ADD_LIBRARY_LINKS %SPXLIB/ipminmsupport libipminmsupport.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
          CWD %IMAGE_TREE/usr/local/lib/ipmi
          CREATE_SOFT_LINK ../libipminmsupport.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX libipminmsupport.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install: