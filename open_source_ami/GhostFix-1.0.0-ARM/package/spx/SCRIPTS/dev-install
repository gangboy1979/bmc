# Install Script

build-install:
	MKDIR_NOCLEAN %IMAGE_TREE/usr/local/lib
	COPY_FILE ./libGhostFix.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %IMAGE_TREE/usr/local/lib
	COPY_FILE ./ld.so.preload %IMAGE_TREE/etc/
	ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libGhostFix.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install:
