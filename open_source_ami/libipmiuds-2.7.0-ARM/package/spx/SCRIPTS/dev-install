# Install Script

build-install:
	COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmilocal.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		CWD %IMAGE_TREE/usr/local/lib/ipmi
		CREATE_SOFT_LINK ../libipmilocal.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX libipmilocal.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install:
