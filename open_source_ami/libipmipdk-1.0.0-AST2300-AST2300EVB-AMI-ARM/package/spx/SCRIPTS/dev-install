# Install Script

build-install:
   MKDIR_NOCLEAN %SPXLIB/ipmipdk
   COPY_DIR    ./ %IMAGE_TREE/usr/local/lib/
   ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmipdk.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
   COPY_FILE ./libipmipdk.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/ipmipdk
   ADD_LIBRARY_LINKS %SPXLIB/ipmipdk libipmipdk.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
   
debug-install:
