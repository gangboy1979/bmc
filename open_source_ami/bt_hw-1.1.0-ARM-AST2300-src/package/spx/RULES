#Extract Source in %SOURCE
extract_source:

#Build Source in %BUILD
build_source:
	CWD %BUILD/%PACKAGE/data
	INVOKE_MAKE

#Clean Source
clean_source:
	CWD %BUILD/%PACKAGE/data
	INVOKE_MAKE clean

#-----------------Mandatory Rules for every binary Package ----------------------------
#Rules to create bt_hw package
build_package-bt_hw:
	PACK_SPX ./ %BUILD/%PACKAGE/data/bt_hw.ko
