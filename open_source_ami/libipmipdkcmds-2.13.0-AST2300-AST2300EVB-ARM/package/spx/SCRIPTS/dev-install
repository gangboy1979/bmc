# Install Script

build-install:
		MKDIR_NOCLEAN %SPXLIB/ipmipdkcmds
		COPY_DIR ./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmipdkcmds.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		COPY_FILE ./libipmipdkcmds.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/ipmipdkcmds
		ADD_LIBRARY_LINKS %SPXLIB/ipmipdkcmds libipmipdkcmds.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX

debug-install:
