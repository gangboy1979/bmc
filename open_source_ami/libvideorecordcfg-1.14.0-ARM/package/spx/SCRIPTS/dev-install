# Install Script

build-install:
		COPY_DIR	./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libvideorecordcfg.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		MKDIR_NOCLEAN    %IMAGE_TREE/usr/local/www/Java/
		COPY_FILE	./video_record.jnlp %IMAGE_TREE/usr/local/www/Java/
		COPY_TO_CONF 	./autorecord.conf
debug-install:
