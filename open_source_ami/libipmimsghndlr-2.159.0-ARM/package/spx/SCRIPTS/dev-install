# Install Script

build-install:
		MKDIR_NOCLEAN %SPXLIB/ipmimsghndlr
		COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmimsghndlr.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		COPY_FILE ./libipmimsghndlr.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/ipmimsghndlr
		ADD_LIBRARY_LINKS %SPXLIB/ipmimsghndlr libipmimsghndlr.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		COPY_FILE ./restoredefaults.sh %IMAGE_TREE/etc
		COPY_FILE ./ArpSwitch.sh %IMAGE_TREE/usr/local/bin

debug-install:


