# Install Script

build-install:
    MKDIR_NOCLEAN  %IMAGE_TREE/usr/local/lib/ipmi/ast2300evb_ami
	MKDIR_NOCLEAN  %IMAGE_TREE/conf/BMC1/ast2300evb_ami
    COPY_FILE ./SDR.dat %IMAGE_TREE/conf/BMC1/ast2300evb_ami
    COPY_FILE ./IPMI.conf %IMAGE_TREE/conf/BMC1/ast2300evb_ami
	MKDIR_NOCLEAN  %IMAGE_TREE/etc/defconfig/BMC1/ast2300evb_ami
    COPY_FILE ./SDR.dat %IMAGE_TREE/etc/defconfig/BMC1/ast2300evb_ami
    COPY_FILE ./IPMI.conf %IMAGE_TREE/etc/defconfig/BMC1/ast2300evb_ami
    COPY_TO_CONF ./BMC.conf
	COPY_FILE ./libipmipar.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX	%IMAGE_TREE/usr/local/lib/ipmi/ast2300evb_ami
	ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib/ipmi/ast2300evb_ami libipmipar.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
	WRITE_TO_FILE ast2300evb_ami %IMAGE_TREE/usr/local/lib/ipmi/platform.conf

debug-install:
