# Install Script

build-install:
		MKDIR_NOCLEAN %SPXLIB/ipmitimer
		COPY_DIR		./ %IMAGE_TREE/usr/local/lib/
		ADD_LIBRARY_LINKS %IMAGE_TREE/usr/local/lib libipmitimer.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX
		COPY_FILE ./libipmitimer.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX %SPXLIB/ipmitimer
		ADD_LIBRARY_LINKS %SPXLIB/ipmitimer libipmitimer.so.%PKG_MAJOR.%PKG_MINOR.%PKG_AUX		

debug-install:
